<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block title %}Megustaelsol.com!{% endblock %}</title>
    {# <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>"> #}
    <!-- Site Icons -->
    {% block icons %}
        <link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}" type="image/x-icon"/>
        <link rel="apple-touch-icon" href="{{ asset('images/apple-touch-icon.png') }}">
    {% endblock %}
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block fonts %}
        <link href="https://fonts.googleapis.com/css?family=Rubik:300,300i,400,400i,500,500i,700,700i,900,900i"
              rel="stylesheet">
    {% endblock %}
    {% block stylesheets %}
        {# {{ encore_entry_link_tags('app') }} #}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">
        <!-- Site CSS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/site.css') }}">
        <!-- ALL VERSION CSS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/versions.css') }}">
        <!-- Responsive CSS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/responsive.css') }}">
        <!-- Custom CSS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/custom.css') }}">
        <!-- Cookie consent banner -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/cookieconsent..css') }}">
        {# <link href="https://cdn.jsdelivr.net/gh/orestbida/cookieconsent@v2.8.6/dist/cookieconsent.css" rel="stylesheet" type="text/css" /> #}
    {% endblock %}
    {% block js %}
        {# {{ encore_entry_script_tags('app') }} #}
        <script type="text/javascript" src="{{ asset('js/modernizer.js') }}"></script>
        {# <script type="text/javascript" src="{{ asset('js/add_phone_spaces.js') }}"></script> #}
    {% endblock %}
</head>
<body>
{% block modal %}
    <div class="modal fade" id="login" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header tit-up">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Login</h4>
                </div>
                <div class="modal-body customer-box">
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs">
                        <li><a class="active" href="#Login" data-toggle="tab">Login</a></li>
                        <li><a href="#Registration" data-toggle="tab">Registrate</a></li>
                    </ul>
                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div class="tab-pane active" id="Login">
                            <form role="form" class="form-horizontal" autocomplete="off">
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <label for="email_login">Email</label>
                                        <input type="email" class="form-control" id="email_login" placeholder="Email"
                                               autocomplete="off" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <label for="password_login">Contraseña</label>
                                        <input type="password" class="form-control" name="password_login"
                                               id="password_login"
                                               placeholder="Contraseña" autocomplete="off" required>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-10">
                                        <button type="submit" class="btn btn-dark btn-radius grd1">
                                            Acceso
                                        </button>
                                        <button type="submit" class="btn btn-dark btn-radius grd1"
                                                 onclick="window.location.href='{{ path('home') }}';">
                                            Cancelar
                                        </button>
                                        <a class="for-pwd" href="javascript:;">Olvidaste tu contraseña?</a>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="tab-pane" id="Registration">
                            <form role="form" class="form-horizontal" method="post" autocomplete="off">
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <label for="name">Nombre</label>
                                        <input class="form-control" id="name" name="name" placeholder="Nombre"
                                               type="text">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <label for="email">Email</label>
                                        <input class="form-control" name="email" id="email" placeholder="Email"
                                               type="email">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <label for="phone">Telefono</label>
                                        <input class="form-control" id="phone"
                                               placeholder="Telefono Ej +34 555 55 55 55" type="tel"
                                               pattern="^(\+34|0034|34)?[ -]*(1|2|3|4|5|6|7|8|9)[ -]*([0-9][ -]*){8}"
                                               required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        {# TODO: Add html regex for password #}
                                        <label for="password_registration">Contraseña</label>
                                        <input type="password" class="form-control" name="password_registration"
                                               id="password_registration" placeholder="Password"
                                               autocomplete="off" required>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        {# TODO: Add html regex for password #}
                                        <label for="password_repeat_registration">Repetir contraseña</label>
                                        <input type="password" class="form-control" name="password_repeat_registration"
                                               id="password_repeat_registration"
                                               placeholder="Repite tu contraseña" autocomplete="off" required>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-10">
                                        <button type="button" class="btn btn-warning btn-radius btn-brd grd1">
                                            Registrarse
                                        </button>
                                        <button type="button" class="btn btn-warning btn-radius btn-brd grd1"
                                                onclick="window.location.href='{{ path('home') }}';">
                                            Cancelar
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block preloader %}
    <div id="preloader">
        <div class="loader-container">
            <div class="progress-br float shadow">
                <div class="progress__item"></div>
            </div>
        </div>
    </div>
{% endblock %}
